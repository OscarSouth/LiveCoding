p "urungTulluk" $ do
  let out = 8*9999
  let
  seqP [sync,startTransport,stopTransport out
    ,bar 0 out metronome
    ]

runWith andante

keySig = A \\\ aeolian

(v,v',c,c') = (verse, verse',chorus,chorus')
    where
      verse = fast 2 $ cat[
        "0 [0 ~ . 0] 2 [0 2] . [1 2] [1 ~] 1 [1 ~]" :: Pattern Int,
        "1 [1 ~ . 1] 2 [1 2] . [1 . 0 ~] [0 -1] 0 [0 ~]" :: Pattern Int
        ]
      verse' = "[0 3 . 3 0 [3 4] 0]" :: Pattern Int
      chorus = fast 2 $ cat [
        "2 [4 3] 4 [2 4] . [2 4] 2 4 [4 ~]" :: Pattern Int,
        "3 [2 4] 3 [0 1] . 2 1 0 [0 ~]" :: Pattern Int
        ]
      chorus' = "-2 2 6 5 5 3 4 0" :: Pattern Int

p06 p0 = d07 $ do
  let prog = inKey keySig
  id $
    stack [silence
      ,note "0*4"
      ,cc 1 (lfo saw 0.65 0.69)
      ,cc 74 0.7
      ] |= ch 06
        -- |- note 12
        |+| prog 4 (c
         -- +| "[0 0 . <-5 -3 0 <-5 0>>]*4"
         +| "[0 <-5 -3>]*16"
         -- +| "[4,7,9]*16"
          )

p08 p0 = d08 $ do
  let prog = inKey keySig
  id $
    stack [silence
      ,note "0*16"
      -- ,note "0"
      -- ,note "0" #legato "[0.25!3 0.5]/4"
      ] |= ch 08
        -- |+ note 12
        -- |= legato "1.1!7 0.7"
        |* vel p0
        |+| prog 4 (c'
         -- +| "[7 0 4 6]*16"
         +| "[0 7]*16"
          )

p09 p0 = d09 $ do
  id $
    stack [silence
      ,ruismk minim "[~ cp]"
      -- ,ruismk crotchet "[~ sn]/2"
      ,ruismk crotchet "sb"
      -- ,ruismk 1 "~ ~ bd ~"
      -- ,within (0.1,1) degrade $ ruismk 1 "bd(3,8) ~"
      -- ,ruismk 1 "hats"
      ,ruismk quaver "hh"
      ] |= ch 09
        |* vel p0
        -- >> silence

p10 p0 = d10 $ do
  id $
    stack [silence
      ,hats 1 "16" #vel (fast 4 $ crotchet ~> lfo sine 0.3 0.54)
      ,kit 1 "[bd:echo bd:lead]" #vel 0.8
      ,kit 1 "~ [~!2 bd? ss] [~ bd] ss" #vel 0.6
      ,kit minim "[~ <rm:echos rm:leads>]" #vel 0.7
      -- ,cymbals crotchet "~ rd" #vel 0.6
      ] |= ch 10
        |* vel "[1.1 0.9!7]*4"
        |+ vel 0.3
        |* vel p0
        |+ humanise 0.2
        -- >> silence

p12 p0 = d12 $ do
  let prog = inKey keySig
  let lfo1 = slow 5 $ 0.5 <~ lfo sine 0.01 0.11
  id $
    stack [silence
      ,dPsyTrance
        0.32 -- cutoff
        0.3 -- res
        (2 <~ lfo1) -- contour
        0 --  osc23
        0.1 -- oscmod
        (lfo1*1) -- lfo
        (slow 2 $ 3 <~ lfo1 * 2) -- mod
      ,phrase 1 "w"
      ] |= ch 12
        |+ note 12
        |* vel p0
        |+| prog 4 (v
         +| "[0 0 . <-5 -3 0 <-5 0>>]*4"
         -- +| "[0 2]*16"
          )
          |+ note "0 <0 0 0 [0 1]> 0 0"
          >> silence

p13 p0 = d13 $ do
  let prog = inKey keySig
  id $
    stack [silence
      ,phrase 1 "w"
      -- ,toneA 0.4 1 1
      -- ,toneB 0.7 1 1
      ] |= ch 13
        -- |- note 24
        |* vel p0
        |+| prog 4 (c
         -- +| "[0 0 . <-5 -3 0 <-5 0>>]*4"
         -- +| "[0 2]*16"
         +| "[0]*8"
          )
          |+ note "0 <0 0 0 [0 1]> 0 0"

hush
